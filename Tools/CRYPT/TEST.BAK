/*
! Use the following procedure to decode it;
*/
char *printCopyright(unsigned char *str)
{
  int seed = 17;
  int len;
  static char buf[80];
  char *bf;
  bf = buf;
  len = *str++;
  for (;len; len--) {
    *bf++ = *str ^ seed;
    seed = *str++;
  }
  *bf = 0;
  return buf;
}

