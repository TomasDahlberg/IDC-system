
TypeLang  set     $F00
Attr_Rev  set     $8001
Edition   set     1

          psect   term,TypeLang,Attr_Rev,Edition,0,0

Port      set     $00340008		0 = port 0, 8 = port 1
Vector    set     $1e
IRQLevel  set     $06
Priority  set     $f0			05
Mode      set     $23

main:
          dc.l    Port
          dc.b    Vector
          dc.b    IRQLevel
          dc.b    Priority
          dc.b    Mode

          dc.w    FileMgr
          dc.w    DevDrv
          dc.w    DevCon
          dc.w    0,0,0,0
          dc.w    OptLen

PD_DTP    set     0
PD_UPC    set     0
PD_BSO    set     1
PD_DLO    set     0
PD_EKO    set     0		1
PD_ALF    set     1
PD_NUL    set     0
PD_PAU    set     0

PD_PAG    set     24            Page length
PD_BSP    set     8             Backspace "input" character
PD_DEL    set     24            Delete line character
PD_EOR    set     13            End of record character

PD_EOF    set     27
PD_RPR    set     4
PD_DUP    set     1
PD_PSC    set     23
PD_INT    set     3
PD_QUT    set     2
PD_BSE    set     8
PD_OVF    set     7
PD_PAR    set     0
PD_BAU    set     $07

PD_XON    set     17
PD_XOFF   set     19
PD_Tab    set     9
PD_Tabs   set     4


optField
          dc.b    PD_DTP
          dc.b    PD_UPC
          dc.b    PD_BSO
          dc.b    PD_DLO
          dc.b    PD_EKO
          dc.b    PD_ALF
          dc.b    PD_NUL
          dc.b    PD_PAU

          dc.b    PD_PAG
          dc.b    PD_BSP
          dc.b    PD_DEL
          dc.b    PD_EOR
          dc.b    PD_EOF
          dc.b    PD_RPR
          dc.b    PD_DUP
          dc.b    PD_PSC
          dc.b    PD_INT
          dc.b    PD_QUT
          dc.b    PD_BSE
          dc.b    PD_OVF
          dc.b    PD_PAR
          dc.b    PD_BAU
          dc.w    PD_D2P
          dc.b    PD_XON
          dc.b    PD_XOFF
          dc.b    PD_Tab
          dc.b    PD_Tabs

OptLen    equ     *-optField

          
FileMgr   dc.b    "Scf",0
DevCon    dc.w    4
DevDrv    dc.b    "sc82c684",0,0

PD_D2P    
* dc.b    "n196",0

          ends
